ndleChange} \n        register={register} \n      />\n      <Note />\n      <div className=\"flex-row\">\n        <CreateAccount />\n        <Next />\n      </div>\n    </div>\n  )\n}\n\nconst Heading = () => (\n  <>\n    <center><h1 className=\"heading\">{t('sign-in.sign_in')}</h1></center>\n    <center><h4 className=\"sub-heading\">{t('sign-in.email.subheading')}</h4></center>\n  </>\n);\n\nconst Form = ({ handleSubmit, onSubmit, handleChange, value, register, uniqueEmail }) => (\n  <form onSubmit={handleSubmit(onSubmit)} className=\"signin-form email-form\" noValidate>\n    <input \n      {...register(\"email\", {\n        required: true,\n        minLength: 2,\n        validate: {\n          checkEmail: async () => await uniqueEmail(),\n        }\n      })}\n      autoComplete=\"off\"\n      name=\"email\"\n      type=\"text\"\n      value={value.email}\n      onChange={handleChange}\n      className=\"input-email\"\n    />\n    <span className=\"input-placeholder\">\n      {t('sign-in.email.form_placeholder')}\n    </span>\n  </form>\n);\n\nconst Note = () => (\n  <div className=\"note\">\n    {t('sign-in.email.note')} \n    <br/>\n    <a className=\"button-blue\" href=\"https://support.google.com/chrome/answer/6130773?hl=en\" rel=\"noreferrer\" target=\"_blank\">\n      {t('learn_more')}\n    </a>\n  </div>\n);\n\nconst CreateAccount = () => (\n  <Link to=\"/signup/\">\n    <button className=\"create-account\">\n      {t('sign-in.email.create_account')}\n    </button>\n  </Link>\n);\n","/home/roqa/Documents/Git/Google/src/pages/SignIn/Password/index.jsx",[],[],"/home/roqa/Documents/Git/Google/src/pages/MyAccount/Home/index.jsx",[],[],"/home/roqa/Documents/Git/Google/src/themes/mode/dark.js",[],[],"/home/roqa/Documents/Git/Google/src/themes/mode/light.js",[],[],"/home/roqa/Documents/Git/Google/src/themes/mode/global.js",[],[],"/home/roqa/Documents/Git/Google/src/themes/toggle/useTheme.js",[],[],"/home/roqa/Documents/Git/Google/src/themes/toggle/toggle.jsx",[],[